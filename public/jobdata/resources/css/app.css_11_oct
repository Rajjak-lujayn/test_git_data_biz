.data_table-section  thead {     }

.data_table-section table th, td:last-child {border-right: none; }

.data_table-section thead th:last-child {border-right: none !important; }

.data_table-section thead th { border-right: 1px solid #C5C5C5; text-align: left;}

.data_table-section table td:last-child { border-right: none; width: 90px;}

.data_table-section table td { border-right: 1px solid #C5C5C5; }

