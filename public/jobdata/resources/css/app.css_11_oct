.filter_section .container-fluid {
    
    
    
padding: 0;}

.filter_section .field input.form-control, .filter_section .field select {
    border: 1px solid #DCDCDC;
    border-radius: 100px;
    height: 40px;
    padding: 0 12px;
    line-height: normal;
    box-shadow: 0 6px 10px rgb(0 0 0 / 12%);
    font-size: 14px;
    text-transform: uppercase;
    font-weight: 600;
    color: #000;
}

.filter_section .field select {
    width: 100%;
    max-width: 250px;
}

.filter_section .field input.form-control::-moz-placeholder {
    color: #000;
}

.filter_section .field input.form-control:-ms-input-placeholder {
    color: #000;
}

.filter_section .field input.form-control::placeholder {
    color: #000;
}

.filter_section .field input.form-control::-webkit-input-placeholder {
    /* Edge */
    color: #000;
}

.filter_section .field input.form-control:-ms-input-placeholder {
    /* Internet Explorer 10-11 */
    color: #000;
}

.filter_section .field {
    padding: 0 5px;
    display: flex;
    align-items: center;
}

.filter_section {
    
    
}

.filter_section .filter_wrapper {
    
    
}

.filter_section .field .radio_btn {
    display: flex;
    border: 1px solid #DCDCDC;
    border-radius: 100px;
    height: 40px;
    padding: 0 12px;
    line-height: normal;
    box-shadow: 0 6px 10px rgb(0 0 0 / 12%);
    font-size: 14px;
    text-transform: uppercase;
    font-weight: 600;
    color: #000;
    align-items: center;
}

.filter_section .field .radio_btn .radio-grp-wrap {
    display: flex;
}

.filter_section .field .radio_btn .radio-grp-wrap .radio_grp {
    display: flex;
    cursor: pointer;
}

.filter_section .field .radio_btn>label.form-check-label {
    margin: 0 11px 0 0;
}

.filter_section .field .radio_btn .radio-grp-wrap .radio_grp:not(:last-child) {
    margin-right: 8px;
}

.filter_section .field .radio_btn .radio-grp-wrap .radio_grp label.form-check-label {
    font-weight: normal;
    margin-left: 3px;
}

.filter_section .field .btn_wrapper {
    display: flex;
}

.filter_section .field .btn_wrapper span.btn {
    margin: 0 !important;
}

.filter_section .field select {
    cursor: pointer;
}

.header_pagination .container-fluid {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.header_pagination {
    background: #222F5D;
    padding: 5px 0;
}

.header_pagination span {
    color: #fff;
    font-size: 12px;
    font-weight: 600;
    text-transform: uppercase;
}

.header_pagination span strong {
    font-weight: 500;
}

.header_pagination select {
    border: none;
    border-radius: 6px;
    height: 36px;
    line-height: normal;
    display: inline-flex;
    padding: 0 0px 0 8px;
    box-sizing: border-box;
    min-width: 52px;
    max-width: 52px;
    margin: 0 9px;
    outline: none;
    cursor: pointer;
}

.header_pagination span:not(:last-child) {
    margin: 0 12px 0 0;
}

.header_pagination .button_wrapper .btn {
    margin: 0 !important;
    border-radius: 3px;
    background: #fff;
    color: #222F5D;
    border: none;
    padding: 6px 15px;
    line-height: normal;
    font-size: 12px;
    letter-spacing: 0;
    text-transform: uppercase;
    font-weight: 800;
    box-shadow: none;
    cursor: pointer;
}

.header_pagination .button_wrapper span.btn:not(:last-child) {
    margin: 0 10px 0 0 !important;
}

header.main_header a.navbar-brand img {
    max-width: 150px;
}

